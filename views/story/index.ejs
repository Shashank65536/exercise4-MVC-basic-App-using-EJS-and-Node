<%- include('../partials/header.ejs')%>
<div class = "content">

    <%if(stories.length){%>
        <p>Select a story to read</p>
        <%console.log('I am in the ejs template and length is ' + stories.length )%>
        <%stories.forEach((obj) =>{%>
            <%console.log('title'+obj.title)%>
            <a href = "/stories/<%=obj.id%>"><p><%= obj.title%></p></a>
            <!-- <p><%=obj.title%></p> -->
        <%});
    } else{%>
        <p>There are no stories to display</p>
    <%}%>
</div>

